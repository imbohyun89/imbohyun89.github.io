<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wt.mapper.Pref_LocMapper">
<resultMap type="prefloc" id="preflocResult">
		<result column="num" property="num"/>
		<result column="city_num" property="city_num"/>
		<result column="user_id" property="user_id"/>
</resultMap>
	
	<select id="select" parameterType="java.lang.Integer" resultMap="preflocResult">
		SELECT * FROM WT_PREF_LOC WHERE num=#{obj}
	</select>
	<select id="selectall" resultType="prefloc">
		SELECT * FROM WT_PREF_LOC
	</select>
	<select id="selectid" parameterType="String" resultType="prefloc">
		SELECT * FROM WT_PREF_LOC WHERE user_id=#{obj}
	</select>
	
	<insert id="insert" parameterType="prefloc">
		INSERT INTO WT_PREF_LOC VALUES(SEQ_PREF_LOC.nextVal,#{city_num},#{user_id})
	</insert>
	<update id="update" parameterType="prefloc">
		UPDATE WT_PREF_LOC SET city_num=#{city_num},user_id=#{user_id} WHERE num=#{num}
	</update>
	<delete id="delete" parameterType="java.lang.Integer">
		DELETE FROM WT_PREF_LOC WHERE num=#{obj}
	</delete>
	
</mapper>






